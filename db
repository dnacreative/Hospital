create database hospital_db; # Make a new database for our use.

CREATE USER 'test'@'localhost' IDENTIFIED BY 'test1234';

GRANT ALL PRIVILEGES ON *.* TO 'test'@'localhost' WITH GRANT OPTION;
grant select, insert, update, delete on hospital_db.* TO 'test'@'localhost' IDENTIFIED BY 'test1234';

use hospital_db;
create table members (username varchar(25) primary key, password char(32), admin boolean);
create table patients (fname varchar(25), lname varchar(25), location varchar(25), dob varchar(25));
create table minimums(location varchar(25), meal varchar(50), min int);
create table A01 (meal varchar(50), stock int, snack int);
create table Q01 (meal varchar(50), stock int, snack int);
create table Lunetten (meal varchar(50), stock int, snack int);
create table log(action varchar(25), user varchar(25), location varchar(25), time varchar(25), first varchar(25), last varchar(25), meal
varchar(50), date varchar(10), quanity int, comment varchar(50), id int not null AUTO_INCREMENT, primary key (id));

create table weekly (last_id int);
insert into weekly(last_id) values(1);

insert into members (username, password, admin) values("admin", "f5d1278e8109edd94e1e4197e04873b9", 1);

